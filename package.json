{
  "name": "nightlydigest-client-function",
  "version": "1.0.0",
  "description": "cloud function for the nightly digest api",
  "main": "dist/index.js",
  "repository": "https://github.com/lsst-epo/nightlydigest-client-function.git",
  "author": "Kevin Wong <kevin.wong@noirlab.edu>",
  "license": "MIT",
  "scripts": {
    "build": "tsc",
    "gcp-build": "npm run build",
    "lint": "npx eslint .",
    "mutate": "npx stryker run",
    "prepare": "husky",
    "prestart": "npm run build",
    "test": "jest",
